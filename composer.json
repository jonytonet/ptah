{
    "name": "jonytonet/ptah",
    "description": "Ptah â€” Laravel scaffolding generator. CRUD, DTOs, Repositories, Services, Livewire 3, Tailwind v4.",
    "type": "library",
    "keywords": ["laravel", "scaffolding", "crud", "generator", "livewire", "tailwind"],
    "license": "MIT",
    "authors": [
        {
            "name": "jonytonet",
            "email": "jonytonet@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0|^12.0",
        "livewire/livewire": "^3.0"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0|^10.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "Ptah\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Ptah\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ptah\\PtahServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
